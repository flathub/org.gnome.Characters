{
    "app-id": "org.gnome.Characters",
    "runtime": "org.gnome.Platform",
    "runtime-version": "41",
    "command": "gnome-characters",
    "sdk": "org.gnome.Sdk",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--metadata=X-DConf=migrate-path=/org/gnome/Characters/"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/share/pkgconfig",
        "/share/aclocal",
        "/man",
        "/share/man",
        "/share/gtk-doc",
        "*.la",
        "*.a",
        "/share/doc",
        "/share/gir-1.0"
    ],
    "modules": [
        {
            "name": "gnome-desktop",
            "buildsystem": "meson",
            "config-opts": [
                "-Ddebug-tools=false",
                "-Dudev=disabled"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnome-desktop/42/gnome-desktop-42.0.tar.xz",
                    "sha256": "f3caa293a5e86f6ccad18f817273db1c67061e680d79d839aa8a7528e5bb26d6",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "gnome-desktop"
                    }
                }
            ]
        },
        {
            "name": "libunistring",
            "cleanup": [
                "/share"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.gnu.org/gnu/libunistring/libunistring-0.9.10.tar.xz",
                    "sha256": "eb8fb2c3e4b6e2d336608377050892b54c3c983b646c561836550863003c05d7"
                }
            ]
        },
        {
            "name": "gnome-characters",
            "buildsystem": "meson",
            "config-opts": [
                "--buildtype=release"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnome-characters/42/gnome-characters-42.0.tar.xz",
                    "sha256": "5da0ffb816f8a6b44f303744ca827a34080118c1c938d8d79af17b7fe67980f8",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "gnome-characters"
                    }
                }
            ]
        }
    ]
}