{
    "app-id" : "org.gnome.Characters",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "40",
    "command" : "gnome-characters",
    "sdk" : "org.gnome.Sdk",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--metadata=X-DConf=migrate-path=/org/gnome/Characters/"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/share/pkgconfig",
        "/share/aclocal",
        "/man",
        "/share/man",
        "/share/gtk-doc",
        "*.la",
        "*.a",
        "/share/doc",
        "/share/gir-1.0"
    ],
    "modules" : [
        {
            "name" : "gnome-desktop",
            "buildsystem" : "meson",
            "config-opts" : [
                "-Ddebug-tools=false",
                "-Dudev=disabled"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url": "https://download.gnome.org/sources/gnome-desktop/40/gnome-desktop-40.0.tar.xz",
                    "sha256": "20abfd3f831e4e8092b55839311661dc73b2bf13fc9bef71c4a5a4475da9ee04"
                }
            ]
        },
        {
            "name" : "libunistring",
            "cleanup" : [
                "/share"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://ftp.gnu.org/gnu/libunistring/libunistring-0.9.10.tar.xz",
                    "sha256" : "eb8fb2c3e4b6e2d336608377050892b54c3c983b646c561836550863003c05d7"
                }
            ]
        },        
        {
            "name" : "libhandy",
            "buildsystem" : "meson",
            "config-opts" : [
                "-Dexamples=false",
                "-Dprofiling=false",
                "-Dglade_catalog=disabled",
                "-Dintrospection=enabled",
                "-Dtests=false",
                "-Dvapi=false"
            ],
            "sources" : [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libhandy/1.2/libhandy-1.2.0.tar.xz",
                    "sha256": "39f590ae20910e76fe1c0607b2ebe589750f45610d6aeec5c30e2ee602a20b25"
                }
            ]
        },
        {
            "name" : "gnome-characters",
            "buildsystem" : "meson",
            "config-opts" : [
                "--buildtype=release"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://download.gnome.org/sources/gnome-characters/40/gnome-characters-40.0.tar.xz",
                    "sha256" : "0a62f5fd56592d0f3eb7be4b132db796c998d7be08ed67e4bf7f059138515d7c"
                }
            ]
        }
    ]
}
